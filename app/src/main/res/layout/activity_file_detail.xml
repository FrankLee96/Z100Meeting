<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gzz100.Z100_HuiYi.meetingManage.fileManage.fileDetailManage.FileDetailActivity">
    <com.gzz100.Z100_HuiYi.widget.NavBarView
        android:id="@+id/id_file_detail_tbv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </com.gzz100.Z100_HuiYi.widget.NavBarView>
    <FrameLayout
        android:layout_below="@id/id_file_detail_tbv"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/id_content"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="内容"
                android:gravity="center"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/id_iv_slide_to_right"
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/file_detail_slide_to_right"/>
        <include
            android:id="@+id/id_slide_layout"
            layout="@layout/file_detail_slide_layout"/>
    </FrameLayout>
</RelativeLayout>
